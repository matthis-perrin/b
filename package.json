{
  "name": "b",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "ncu": "ts-node --esm ./src/ncu.ts",
    "build": "NODE_ENV=development webpack build",
    "watch": "NODE_ENV=development webpack watch",
    "project": "yarn build && yarn start && cd packages/project/ && yarn install && cd ../../ && node packages/project/index.js",
    "start": "node dist/index.js",
    "postinstall": "node install.js",
    "publish-all": "for d in ./packages/* ; do (cd \"$d\" && npm publish --access public && cd ..); done; rm -rf **/yarn.lock; yarn install"
  },
  "prettier": "@matthis/prettier-config",
  "eslintConfig": {
    "extends": [
      "@matthis/eslint-config-node"
    ],
    "rules": {
      "no-console": "off"
    },
    "overrides": [
      {
        "files": "src/webpack/*.ts",
        "rules": {
          "unicorn/filename-case": "off"
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.x",
    "@babel/preset-env": "7.19.x",
    "@babel/preset-react": "7.18.x",
    "@babel/preset-typescript": "7.18.x",
    "@matthis/eslint-config-node": "latest",
    "@matthis/prettier-config": "latest",
    "@matthis/tsconfig-node": "latest",
    "@matthis/webpack-node": "latest",
    "@types/node": "18.7.x",
    "@types/prettier": "2.7.x",
    "@types/prompts": "^2.0.14",
    "@types/semver": "7.3.x",
    "babel-loader": "8.2.x",
    "babel-plugin-react-remove-properties": "0.3.x",
    "chokidar": "3.5.x",
    "fork-ts-checker-webpack-plugin": "7.2.x",
    "html-webpack-plugin": "5.5.x",
    "package-json": "8.1.x",
    "semver": "7.3.x",
    "source-map-loader": "4.0.x",
    "terser-webpack-plugin": "5.3.x",
    "webpack": "5.74.x",
    "webpack-cli": "4.10.x",
    "webpack-dev-server": "4.11.x"
  },
  "dependencies": {
    "prompts": "2.4.x"
  }
}
